<strings>

    <string id="findByUsername">
        select * from ${schemaTableName} where 1=1 and id = #{username}
    </string>


    <string id="findByUsername2">
        <![CDATA[
                select * from ${schemaTableName} where 1=1
                and id=#{username}
        ]]>
    </string>

    <string id="findByUsername3">
        select * from ${schemaTableName} where 2=2 and ${_conditions}
    </string>

</strings>